@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Profile</title>
    <!-- <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css">-->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="~/Content/Profile_Data/bootstrap-4.3.1-dist/css/bootstrap.css" type="text/css">
    <link rel="stylesheet" href="~/Content/Profile_Data/css/style.css" type="text/css">
</head>
<body>
    <div class="container fixed-top">
        <div class="no-gutters">
            <div class="col-lg-12 col-md-12">
                <div class="col-lg-12 col-md-12">
                    <div class="row text-center text-white">
                        <div class="p-3 bg-dark font-div">
                            <a href="#basicInfo" class="text-white no-underline">
                                <i class="fa fa-home"></i> Basic Info
                            </a>
                        </div>
                        <div class="p-3 bg-danger font-div">
                            <a href="#education" class="text-white no-underline">
                                <i class="fa fa-graduation-cap"></i> Education
                            </a>
                        </div>
                        <div class="p-3 bg-primary font-div">
                            <a href="#skills" class="text-white no-underline">
                                <i class="fa fa-line-chart"></i> Skills
                            </a>
                        </div>
                        <div class="p-3 bg-success font-div">
                            <a href="#projects" class="text-white no-underline">
                                <i class="fa fa-tasks"></i> Projects
                            </a>
                        </div>
                        <div class="p-3 bg-purple font-div">
                            <a href="#workExperience" class="text-white no-underline">
                                <i class="fa fa-folder"></i> Work Ex.
                            </a>
                        </div>
                        <div class="p-3 bg-warning font-div">
                            <a href="#contact" class="text-white no-underline">
                                <i class="fa fa-envelope"></i> Contact
                            </a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Basic Information -->
    <div class="container pseudo-element" id="basicInfo">
        <div class="no-gutters">
            <div class="col-lg-12 col-md-12">
                <div class="p-4 bg-dark text-white">
                    <div class="row">
                        <div class="col-lg-9 col-md-7 col-sm-6">
                            <h1 class="display-5 d-block">@ViewBag.profName</h1>
                            <h6 class="display-5" style="opacity: .5;">@ViewBag.profRole</h6>
                        </div>
                        <div class="col-lg-3 col-md-5 col-sm-6" style="opacity: .65;">
                            <div class="row">
                                <a href="#" class="text-white">
                                    <i class="fa fa-phone"> @ViewBag.profPhone</i>
                                </a>
                            </div>
                            <div class="row">
                                <a href="http://gmail.com" class="text-white">
                                    <i class="fa fa-envelope"> @ViewBag.profEmail</i>
                                </a>
                            </div>
                            <div class="row">
                                <a href="https://in.linkedin.com/in/dummy-link-775982111" class="text-white">
                                    <i class="fa fa-linkedin"> @ViewBag.profLinkedin</i>
                                </a>
                            </div>
                        </div>                        
                    </div>
                </div>
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 bg-summary">
                    <div class="text-white-50 text-center p-2  css-summary">
                        @ViewBag.profSummary
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Education -->
    <div class="container">
        <div id="education" class="pseudo-element">
            <div class="p-3 text-white bg-danger">
                <h2>Educational Details</h2>
            </div>
            <!-- Cards Here -->
            <div class="col-md-12 col-lg-12 bg-light">
                <div id="educationConnectionId" class="row">
                    <div class="col-lg-4 mb-2 mt-2 text-center text-white">
                        <div class="card col-md-9 col-lg-11 offset-lg-1 offset-md-2 bg-card">
                            <div class="card-header">+3</div>
                            <div class="card-body">
                                <h4>Utkal University</h4>
                                <h5>Percentage<label>80%</label></h5>
                            </div>
                            <div class="card-footer">Passing Year - 2017</div>
                        </div>
                    </div>
                    <div class="col-lg-4 mb-2 mt-2 text-center text-white">
                        <div class="card col-md-9 col-lg-11 offset-lg-1 offset-md-2 bg-card">
                            <div class="card-header">+2</div>
                            <div class="card-body">
                                <h4>Utkal University</h4>
                                <h5>Percentage<label>80%</label></h5>
                            </div>
                            <div class="card-footer">Passing Year - 2017</div>
                        </div>
                    </div>
                    <div class="col-lg-4 mb-2 mt-2 text-center text-white">
                        <div class="card col-md-9 col-lg-11 offset-lg-1 offset-md-2 bg-card">
