                        <progress class="progress progress-bar mb-3" id="progBar" value="75" max="100" style="width: 100%"></progress>
                    </div>
                    <div class="col-md-6 col-lg-6">
                        <h4>CSS</h4>
                        <progress class="progress progress-bar mb-3" id="progBar" value="55" max="100" style="width: 100%"></progress>
                    </div>
                    <div class="col-md-6 col-lg-6">
                        <h4>JavaScript</h4>
                        <progress class="progress progress-bar mb-3" id="progBar" value="95" max="100" style="width: 100%"></progress>
                    </div>
                    <div class="col-md-6 col-lg-6">
                        <h4>jQuery</h4>
                        <progress class="progress progress-bar mb-3" id="progBar" value="70" max="100" style="width: 100%"></progress>
                    </div>
                    <div class="col-md-6 col-lg-6">
                        <h4>BootStrap</h4>
                        <progress class="progress progress-bar mb-3" id="progBar" value="45" max="100" style="width: 100%"></progress>
                    </div>
                    <div class="col-md-6 col-lg-6">
                        <h4>C#</h4>
                        <progress class="progress progress-bar mb-3" id="progBar" value="65" max="100" style="width: 100%"></progress>
                    </div>
                    <div class="col-md-6 col-lg-6">
                        <h4>ASP.NET MVC</h4>
                        <progress class="progress progress-bar mb-3" id="progBar" value="90" max="100" style="width: 100%"></progress>
                    </div>
                </div>
            </div>
        </div>
    </div>
